	                            points.push(causeP);
	                            causeI = causeI + 1;
	                        }
	                    }
	                    countryI = countryI + 1;
	                }
	            }
	            regionP.value = Math.round(regionVal / countryI);
	            points.push(regionP);
	            regionI = regionI + 1;
	        }
